package com.newUser;
import com.user.*;

public class newUser extends user {
	private boolean verified;
	
	public newUser(int id, String name, String password, String gmail, String phoneNumber, String role,String verification) {
		super(id, name, password, gmail, phoneNumber, role);
		this.verified=this.updateVerified(verification);
	}

	public boolean updateVerified(String Dbinput){
		if("verified".equals(Dbinput)) {
			return true;
		}else {
			return false;
		}
	}

}
